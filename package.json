{
	"name": "demo-graphile",
	"version": "0.1",
	"description": "demo for graphile, TypeScript",
	"main": "dist",
	"repository": "https://github.com/jason-den/demo-ts-graphile",
	"author": "jason",
	"license": "MIT",
	"private": false,
	"scripts": {
		"lint": "yarn eslint .",
		"lint:fix": "yarn lint --fix",
		"build": "swc src -s -D -d dist",
		"dev": "source .dev.env && NODE_ENV=development && DEBUG=\"graphile-build:warn\" nodemon --ext \"ts,js,json\" -w src --exec \"yarn build && node dist",
		"start": "source .dev.env && NODE_ENV=development && nodemon --ext \"ts,js,json\" -w .. --ignore '*/dist/*' --exec \"yarn build && node dist\""
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.14",
		"@types/helmet": "^4.0.0",
		"@types/jest": "^29.2.0",
		"@types/knex": "^0.16.1",
		"@types/node": "^18.15.0",
		"@types/pg": "^8.6.6",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"prettier": "^2.7.1",
		"ts-jest": "^29.0.3",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@apollo/server": "^4.5.0",
		"@apollo/server-plugin-landing-page-graphql-playground": "^4.0.0",
		"@swc/cli": "0.1.56",
		"@swc/core": "1.2.143",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"graphile-utils": "^4.12.3",
		"graphql-ws": "^5.12.0",
		"helmet": "^6.0.1",
		"knex": "^2.4.2",
		"nodemon": "^2.0.21",
		"pg": "^8.10.0",
		"postgraphile": "4.12.12",
		"postgraphile-apollo-server": "^0.1.1",
		"postgraphile-plugin-connection-filter": "1.0.0",
		"postgraphile-plugin-upload-field": "^1.0.0-alpha.10",
		"ws": "^8.13.0"
	}
}
